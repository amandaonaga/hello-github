import random

resultados = []
i = 0
q = int(input("Este é um simulador de dados. Quantos resultados você gostaria de obter?"))

while q>0:
    i = random.randint(1,6)
    resultados.append(i)
    q = q-1

print(resultados)
x=1
while x<7:
    print("Contagem "+str(x)+": ", resultados.count(x))
    x=x+1

